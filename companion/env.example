# Cursor Telemetry Dashboard - Environment Configuration

# ============================================
# API Security
# ============================================

# Require API authentication (set to 'true' to enable)
REQUIRE_AUTH=false

# API Keys (comma-separated for multiple keys)
# Generate keys with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# API_KEYS=your_secret_api_key_here,another_key_here

# Allow public API access without authentication (not recommended for production)
# PUBLIC_API=true

# ============================================
# AI/Reasoning Engine
# ============================================

# Hugging Face API Token (for AI reasoning features)
HF_TOKEN=your_huggingface_token_here

# OpenRouter API Key (for semantic search embeddings and cluster labeling)
# Get your key at: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# ============================================
# Server Configuration
# ============================================

# Host and port for companion service
HOST=127.0.0.1
PORT=43917

# ============================================
# Research Data Collection
# ============================================

# Enable research data collection and sync
RESEARCH_ENABLED=false

# Research server URL for data submission
RESEARCH_SERVER_URL=https://research.example.com

# Privacy rung level for research data (1=tokens, 2=semantic_edits, 3=functions, 4=module_graph, 5=DAG, 6=motifs/clio)
RESEARCH_RUNG=5

# Sync interval in milliseconds (default: 5 minutes)
RESEARCH_SYNC_INTERVAL=300000

